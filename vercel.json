{
  "rewrites": [
    {
      "source": "/((?!assets|favicon|apple-touch-icon|manifest|sitemap|robots).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    }
  ]
}

